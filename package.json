{
  "name": "@tn-group/mongo-connector",
  "version": "0.0.2",
  "scripts": {
    "precommit": "if git-branch-is develop; then npm test; fi",
    "start": "NODE_ENV=development nodemon src/runner.coffee",
    "test": "DEBUG=mongo-connector NODE_ENV=test nyc mocha",
    "compile": "coffee --output build --compile src"
  },
  "main": "./build/index.js",
  "files": [
    "build"
  ],
  "repository": {
    "type": "git",
    "url": "[git@git.viriciti.com:10022]:vems/mongo-connector.git"
  },
  "author": "pim",
  "nyc": {
    "check-coverage": false,
    "lines": 80,
    "functions": 80,
    "branches": 80,
    "reporter": [
      "html",
      "text"
    ],
    "exclude": [
      "config",
      "src/runner.coffee",
      "config/production.coffee",
      "**/index.coffee",
      "test.coffee"
    ]
  },
  "dependencies": {
    "@tn-group/log": "^3.2.0",
    "debug": "^3.1.0",
    "mongodb-uri": "^0.9.7",
    "mongoose": "^5.1.7",
    "underscore": "^1.9.1"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "coffee-coverage": "^3.0.0",
    "coffee-script": "^1.12.7",
    "coffeescope2": "^0.5.1",
    "husky": "^0.14.3",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2"
  }
}
